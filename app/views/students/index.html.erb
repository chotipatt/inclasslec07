<p style="color: green"><%= notice %></p>

<h1>Students</h1>

<div id="students">
  <% @students.each do |student| %>
    <%= render student %>
    <p>
      <%= link_to "Show this student", student %>
      <% if session[:login] %>
        <%= link_to "Edit Score", aaa_path(student.id) , stu:student %>
      <%end%>
    </p>
  <% end %>
</div>

<%= link_to "New student", new_student_path %>

